#include<xc.h> 
#include<plib/delays.h> 
#include "Config.h" 
#ifndef XTAL_FREQ 
#define _XTAL_FREQ 20000000 
#endif 
void delay_ms(unsigned char t); 
int main() 
{ 
    TRISD = 0; 
    while(1) 
    {  
        PORTD = 0xFF; 
        delay_ms(100); 
        PORTD = 0x00; 
        delay_ms(100); 
    } 
} 
void delay_ms(unsigned char t) 
{ 
    int i; 
    for(i=0;i<t;i++) 
        Delay1KTCYx(5); 
} 
